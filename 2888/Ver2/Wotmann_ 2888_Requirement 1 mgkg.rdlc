          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>TUEV_CN</DataSourceName>
        <CommandText>SELECT s.*, t.Rownumber
  FROM (SELECT Sample.Composite_No,
               Sample.Id                             Sample_Id,
               Sample.Material_No,
               Sample.Pass_Fail_Display,
               Sample.Pass_Fail_Indicator,
               Sample.Page_No,
               Sample_Result.Test_Parameter_Id,
               Param.Name                            Parameter_Name,
               Param.Name_For_Test_Report            Parameter_Name_For_Test_Report,
               Param.Short_Name                      Parameter_Short_Name,
               Sample_Result.Result,
               Sample_Result.Limit_Display,
               Sample_Result.Print_In_Report,
               Sample_Result.Limit_Violation,
               Sample_Result.Method_Id,
               Param.Cas_No,
               Sample_Result.Id                      Result_Id,
               Sample_Result.Method_Version,
               Sample_Result.Sno,
               Sample_Result.Unit                    Unit_Name,
               Sample_Result.Method_Detection_Limit  Mdl,
               Bscoresample.Classification_Statement Method,
               Bscoresample.Weight_Under_100         Weight,
               Sample_Result.Result_Format,
               Sample_Result.X1,
               Sample_Result.X2,
               Sample_Result.X3,
               Sample_Result.Y1,
               Sample_Result.Y2,
               Sample_Result.Y3,
               Sample_Result.Z1,
               Sample_Result.Z2,
               Sample_Result.Z3,
               Sample_Result.Remark_Picture_Path,
               Bscoresample.checklist_condition_id,
Sample.sno sample_sno,
sysdate "REQUIREMENT_FULL_STRING",
sysdate "TEST_PARAM_FULL_STRING",
sysdate "REQUIREMENT_HEADER",
               param.v1                              Parameter_v1,
               param.v2                              Parameter_v2,    
               param.v3                              Parameter_v3
          FROM (SELECT *
                  FROM Bssrep_Report_Sample
                 WHERE Report_Id = :REPORT_ID) SAMPLE
          LEFT JOIN (SELECT t.*, y.Method_Detection_Limit
                      FROM Bssrep_Report_Smpl_Result t
                      LEFT JOIN Bscore_Result y
                        ON y.Id = t.Result_Id
                     WHERE t.Report_Id = :REPORT_ID) Sample_Result
            ON Sample.Id = Sample_Result.Report_Sample_Id
          LEFT JOIN (SELECT *
                      FROM Bstm_Test_Parameter
                     WHERE Id IN (SELECT DISTINCT Test_Parameter_Id
                                    FROM Bssrep_Report_Smpl_Result
                                   WHERE Report_Id = :REPORT_ID)) Param
            ON Param.Id = Sample_Result.Test_Parameter_Id
          LEFT JOIN (SELECT *
                      FROM Bsunit_Unit
                     WHERE Id IN (SELECT DISTINCT Unit_Id
                                    FROM Bssrep_Report_Smpl_Result
                                   WHERE Report_Id = :REPORT_ID)) Unit
            ON Param.Unit_Id = Unit.Id
        
          LEFT JOIN (SELECT *
                      FROM Bscore_Sample s
                     WHERE s.Id IN (SELECT Sample_Id
                                      FROM Bssrep_Report_Sample
                                     WHERE Report_Id = :REPORT_ID)) Bscoresample
            ON Bscoresample.Id = Sample.Sample_Id) s

  LEFT JOIN (SELECT DISTINCT Ceil(Rownum / :Sample_Per_Row) AS Rownumber,
                             Bssrep_Report_Sample.Composite_No,
                             Bssrep_Report_Sample.Material_No
               FROM Bssrep_Report_Sample
              WHERE Report_Id = :REPORT_ID
                AND Bssrep_Report_Sample.Method_New_Id IN
                    (SELECT Id
                       FROM Bssrep_Report_Method_New
                      WHERE Report_Id = :REPORT_ID
                        AND Method_Id = :Method_Id)
              ORDER BY Bssrep_Report_Sample.Composite_No,
                       Bssrep_Report_Sample.Material_No) t
    ON s.Composite_No = t.Composite_No
   AND s.Material_No = t.Material_No</CommandText>
        <QueryParameters>
          <QueryParameter Name=":REPORT_ID">
            <Value>=Parameters!REPORT_ID.Value</Value>
          </QueryParameter>
          <QueryParameter Name=":Sample_Per_Row">
            <Value>=Parameters!SAMPLE_PER_ROW.Value</Value>
          </QueryParameter>
          <QueryParameter Name=":Method_Id">
            <Value>=Parameters!METHOD_ID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
