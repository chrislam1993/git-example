  <Code>Public Function AbbrFixSpelling(ByVal s As String) As String 
'--Coding for transferring Abbreviation formation from Old LIMS to New LIMS--
   Dim strBuilder As New System.Text.StringBuilder(s)
   If s.Contains("Abbreviation:") Then
      strBuilder.Replace("Abbreviation: ", "")	' -Remove "Abbreviation:" with space	 
      strBuilder.Replace("Abbreviation : ", "")	' -Remove "Abbreviation :" with space	
      strBuilder.Replace("Abbreviation:	","")	' -Remove "Abbreviation:" with Tab
      strBuilder.Replace("Abbreviation :	","")	' -Remove "Abbreviation :" with Tab
      strBuilder.Replace("	", "")		' -Remove Tab
      strBuilder.Replace("  ", "")		' -Remove Duoble space
      Return strBuilder.ToString()
      Else : Return s
   End If
End Function
</Code>
